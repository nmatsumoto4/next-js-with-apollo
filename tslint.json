{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-airbnb",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "import-name": false,
      "variable-name": [false],
      "no-function-constructor-with-string-args":false,
      "prettier": [
        true,
        {
          "singleQuote": true,
          "semi": true
        }
      ]
    }
  }