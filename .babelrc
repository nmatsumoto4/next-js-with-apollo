{
  "presets": ["next/babel", "@zeit/next-typescript/babel"],
  "plugins": [
    "@babel/plugin-proposal-export-default-from",
    ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}
